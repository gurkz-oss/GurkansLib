{
  "schemaVersion": 1,
  "id": "gurkanslib",
  "version": "${version}",

  "name": "Gurkan's Lib",
  "description": "a library for Gurkan's mods",
  "authors": ["Gurkan"],
  "contact": {},

  "license": "GPL-3.0",

  "environment": "server",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "me.gurkz.gurkanslib.GurkansLib"
      }
    ]
  },

  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": [
      "@supported_versions@"
    ],
    "fabric-permissions-api-v0": "*",
    "fabric-language-kotlin": ">=1.13.0+kotlin.2.1.0"
  }
}